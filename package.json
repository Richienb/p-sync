{
	"name": "p-sync",
	"version": "0.0.0",
	"description": "Resolve a promise synchronously.",
	"repository": "https://github.com/Richienb/p-sync.git",
	"author": "Richie Bendall <richiebendall@gmail.com>",
	"license": "MIT",
	"main": "index.js",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"lint": "xo",
		"test": "yarn lint && ava"
	},
	"keywords": [
		"promise",
		"async",
		"sync",
		"synchronous",
		"resolve",
		"value",
		"get"
	],
	"dependencies": {
		"deasync": "^0.1.19",
		"p-is-promise": "^3.0.0"
	},
	"devDependencies": {
		"ava": "^3.5.0",
		"eslint-config-richienb": "^0.3.0",
		"execa": "^4.0.0",
		"normalize-newline": "^3.0.0",
		"xo": "^0.26.1"
	},
	"resolutions": {
		"eslint": "^6.8.0"
	},
	"xo": {
		"extends": "richienb/node",
		"rules": {
			"promise/prefer-await-to-then": 0,
			"promise/always-return": 0
		}
	}
}
